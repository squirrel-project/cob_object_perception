<package format="2">
  <name>cob_object_perception</name>
  <version>0.1.0</version>

  <description>
    This meta-package includes open source software for object recognition. Objects may be marker patterns or written text
  </description>

  <maintainer email="richard.bormann@ipa.fraunhofer.de">Richard Bormann</maintainer>
  <maintainer email="josh@ipa.fhg.de">Joshua Hampp</maintainer>

  <license>LGPL</license>
  <url type="website">http://ros.org/wiki/cob_object_perception</url>

  <buildtool_depend>catkin</buildtool_depend>

  <exec_depend>cob_fiducials</exec_depend>
  <exec_depend>cob_marker</exec_depend>
  <exec_depend>cob_object_categorization</exec_depend>
  <exec_depend>cob_object_detection_fake</exec_depend>
  <exec_depend>cob_object_recording</exec_depend>
  <exec_depend>cob_read_text</exec_depend>
  <!--exec_depend>cob_surface_classification</exec_depend-->
  <exec_depend>cob_texture_classification</exec_depend>
  <!--exec_depend>libvlfeat</exec_depend>
  <exec_depend>libzxing</exec_depend-->

  <export>
    <metapackage />
  </export>
</package>
